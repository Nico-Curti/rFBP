DOXYFILE_ENCODING = UTF-8
PROJECT_NAME = rFBP
OUTPUT_DIRECTORY = @CMAKE_SOURCE_DIR@/docs/sphinxtext/source/
CREATE_SUBDIRS = NO
OUTPUT_LANGUAGE = English
ALWAYS_DETAILED_SEC = NO
OPTIMIZE_OUTPUT_FOR_C = YES
EXTRACT_PRIVATE = NO
EXTRACT_STATIC = NO
HIDE_UNDOC_MEMBERS = YES
HIDE_UNDOC_CLASSES = YES
FILE_PATTERNS = *.cpp *.h *.hpp
EXCLUDE = ./include/parse_args.h ./include/cmd.h ./include/errors.h ./include/utils.h ./include/spline.h
USE_MATHJAX = YES
FORMULA_TRANSPARENT = YES
FORMULA_FONTSIZE = 10
GENERATE_HTML = NO
GENERATE_LATEX = NO
GENERATE_XML = YES
XML_OUTPUT = doxydoc
# COMPACT_XML = YES
# RTF_HYPERLINKS = NO
BUILTIN_STL_SUPPORT = YES
EXTRACT_ALL = YES
INPUT = @CMAKE_CURRENT_SOURCE_DIR@/../include/
RECURSIVE = YES
HIDE_UNDOC_RELATIONS = YES
